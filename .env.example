# Database connection string
POSTGRES_URL=postgresql://127.0.0.1:5432/your-database-name
# Used to encrypt JWT tokens
PAYLOAD_SECRET=YOUR_SECRET_HERE
# Used to configure CORS, format links and more. No trailing slash
NEXT_PUBLIC_SERVER_URL=http://localhost:3000
# Secret used to authenticate cron jobs
CRON_SECRET=YOUR_CRON_SECRET_HERE
# Used to validate preview requests
PREVIEW_SECRET=YOUR_SECRET_HERE

# Background Removal API (Photoroom)
# Get your API key at https://www.photoroom.com/api
PHOTOROOM_API_KEY=YOUR_PHOTOROOM_API_KEY

# FingerprintJS Pro (One-Shot Engine)
# Get your API key at https://fingerprint.com
NEXT_PUBLIC_FINGERPRINT_API_KEY=YOUR_FINGERPRINT_API_KEY

# Stripe (Subscriptions)
# Get your keys at https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=YOUR_STRIPE_SECRET_KEY
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY
STRIPE_WEBHOOK_SECRET=YOUR_STRIPE_WEBHOOK_SECRET

# Resend (Email & Marketing)
# Get your API key at https://resend.com/api-keys
# Used for transactional emails and marketing contact sync
RESEND_API_KEY=YOUR_RESEND_API_KEY

# Sentry Integration (Mobile App Error Tracking)
# Get your API token at https://sentry.io/settings/account/api/auth-tokens/
SENTRY_API_TOKEN=YOUR_SENTRY_API_TOKEN
SENTRY_WEBHOOK_SECRET=YOUR_SENTRY_WEBHOOK_SECRET
SENTRY_ORG_SLUG=your-org-slug

# RudderStack Analytics (Customer Data Platform)
# Get your keys at https://app.rudderstack.com/sources
# Client-side (website) tracking
NEXT_PUBLIC_RUDDERSTACK_WRITE_KEY=YOUR_RUDDERSTACK_WRITE_KEY
NEXT_PUBLIC_RUDDERSTACK_DATA_PLANE_URL=YOUR_RUDDERSTACK_DATA_PLANE_URL
# Server-side tracking (optional, falls back to client-side keys if not set)
# Create a separate Node.js source in RudderStack for server-side events
RUDDERSTACK_SERVER_WRITE_KEY=YOUR_RUDDERSTACK_SERVER_WRITE_KEY
RUDDERSTACK_DATA_PLANE_URL=YOUR_RUDDERSTACK_DATA_PLANE_URL

# Vercel KV (Rate Limiting & Caching)
# Required for serverless rate limiting on Vercel
# Create at: https://vercel.com/dashboard/stores?type=kv
# These are auto-populated when you link a KV store to your project
KV_REST_API_URL=YOUR_KV_REST_API_URL
KV_REST_API_TOKEN=YOUR_KV_REST_API_TOKEN

# Redis (General Caching - Optional)
# Falls back to in-memory cache if not configured
# Format: redis://[[username][:password]@][host][:port][/db-number]
REDIS_URL=redis://localhost:6379

# Mixpanel Analytics Dashboard
# Option 1: Service Account authentication (recommended)
MIXPANEL_SERVICE_ACCOUNT=YOUR_SERVICE_ACCOUNT
MIXPANEL_SECRET=YOUR_SERVICE_SECRET
# Option 2: API Secret authentication (simpler setup)
MIXPANEL_API_SECRET=YOUR_API_SECRET
MIXPANEL_PROJECT_ID=YOUR_PROJECT_ID

# RevenueCat (Subscription Management)
# Get your API key at https://app.revenuecat.com/api-keys
REVENUECAT_API_KEY=YOUR_REVENUECAT_API_KEY

# Statsig (Feature Flags & Experiments)
# Get your API key at https://console.statsig.com/api_keys
STATSIG_SERVER_SECRET=YOUR_STATSIG_SERVER_SECRET
STATSIG_CONSOLE_API_KEY=YOUR_STATSIG_CONSOLE_API_KEY